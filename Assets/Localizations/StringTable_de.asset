  m_TableData:
  - m_Id: 150805618688
    m_Localized: "In dieser Szene haben Sie die Gelegenheit, das antike Forum von
      Sufetula zu erkunden, das Herzst\xFCck des politischen und religi\xF6sen Lebens
      der r\xF6mischen Stadt. \r\n\r\nDas Forum von Sufetula ist eines der am besten
      erhaltenen Beispiele in ganz Nordafrika. Diese Szene zeigt, wie es um das 3."
    m_Metadata:
      m_Items: []
  - m_Id: 50862562418688
    m_Localized: "Zur\xFCck"
    m_Metadata:
      m_Items: []
  - m_Id: 51153810694144
    m_Localized: "Men\xFC"
    m_Metadata:
      m_Items: []
  - m_Id: 53094485794816
    m_Localized: Sprache
